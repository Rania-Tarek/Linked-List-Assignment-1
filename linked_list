#include<iostream>
using namespace std;
class Node {
public:
	char data;
	Node* next;
};
int main() {
	Node* first = new Node();
	Node* second = new Node();
	Node* third  = new Node();
	Node* fourth = new Node();
	Node* fifth  = new Node();
	first->data = 'R';
	first->next = second;
	second->data = 'A';
	second->next = third;
	third->data = 'N';
	third->next = fourth;
	fourth->data = 'I';
	fourth->next = fifth;
	fifth->data = 'A';

	cout << first->data << endl;

	cout << second->data<< endl;
  
	cout << third-> data << endl;
	 
	cout << fourth->data << endl; 

	cout <<fifth->data << endl; 
	


	return 0;
}
